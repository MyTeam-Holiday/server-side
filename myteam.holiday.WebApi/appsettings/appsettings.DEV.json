{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "localhost;*.myteam.holiday",

  "PermittedHosts": {
    "Whitelist": [ "*" ]
  },

  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://localhost:5000"
      },
      "Https": {
        "Url": "https://localhost:5001"
      }
    }
  },

  "BuildConfig": "DEV",

  "ConnectionStrings": {
    "DefaultConnection": "server=[ip];user id=[user];password=[pwd];database=[dbname]"
  },

  "EmailConfig": {
    "EmailAddress": "myteamholiday@gmail.com",
    "EmailHost": "smtp.gmail.com",
    "EmailPassword": "[pwd]"
  }
}
