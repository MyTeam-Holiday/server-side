{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "AllowedHosts": "localhost;*.{clientdomain}",

  "PermittedHosts": {
    "Whitelist": [ "*" ]
  },

  "Kestrel": {
    "EndPoints": {
      "Http": {
        "Url": "http://dev.{clientdomain}:5000"
      },
      "Https": {
        "Url": "https://dev.{clientdomain}:5001"
      }
    }
  },

  "BuildConfig": "DEV",

  "ConnectionStrings": {
    "DefaultConnection": "server={server_ip};user id={db_user_id};password={db_password};database={db_name}"
  },

  "EmailConfig": {
    "EmailHost": "{email_host}",
    "EmailAddress": "{email_address}",
    "EmailPassword": "{email_password}"
  },

  "Auth0": {
    "Domain": "{clientdomain}",
    "ClientId": "{client_id}",
    "ClientSecret": "{client_secret}"
  }
}
